  <link href="font/font.css" type="text/css" rel="stylesheet" />
  <link href="css/style.css" type="text/css" rel="stylesheet" />
  <link href="css/media-query.css" type="text/css" rel="stylesheet" />

</head>

<body data-anm=".anm">

  <div class="wrapper">
    <%- include('layout/header') %>

    <div id="main-content">

      <div class="inner-header">
        <div class="container">
          <h1>Contact Us</h1>
          <div class="breadcrums">
            <a href="/">Home</a>
            <a href="javascript:void(0);">Contact Us</a>
          </div>
        </div>
        <div class="curv-svg">
          <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px"
            y="0px" viewBox="0 0 1439 39.7" style="enable-background:new 0 0 1439 39.7;" xml:space="preserve">
            <path
              d="M749.5,39.7c-12,0-24,0-36,0c-1.5-0.2-2.9-0.4-4.4-0.5c-43.6-1.7-85.9-7.6-127.2-16.6c-18-3.9-36.3-7-55.3-7.9
                        c-36.2-1.8-71.7,1.4-107.2,5.5c-22.7,2.6-45.3,6-68.2,7.9c-28.4,2.3-57,3.5-85.5,4.8c-42.2,1.8-84.2,0.4-125.7-5.1
                        c-41.6-5.5-83.5-8.3-126-6.6C9.5,21.4,4.9,21.6,0,21.8C0,14.3,0,7.1,0,0c479.7,0,959.3,0,1439,0c0,6.8,0,13.7,0,20.5
                        c-20.1,2.9-40.1,6-60.3,8.7c-36.2,4.7-72.5,5.4-109.2,1.8c-35.1-3.4-69.6-8.4-103.8-14.1c-32.9-5.5-66-10.1-99.9-11.7
                        c-38.9-1.9-77.8-2.2-116.6,0.6c-35.9,2.6-70.7,7.6-103.2,18.3c-22.5,7.4-46.7,11.6-71.8,13.8C765.9,38.5,757.7,39.1,749.5,39.7z" />
          </svg>

        </div>
      </div>

      <section class="contact-section inner-about-section">
        <!-- <div class="pos-abs red-green-chilli" data-aos="fade-right">
          <img src="images/assets/chilli.png" alt="Maruti Munchies" />
        </div> -->
        <div class="pos-abs abt-wheat-abs" data-aos="fade-left">
          <!-- <img src="images/assets/wheat-9.png" alt="Maruti Munchise" /> -->
          </div>
        <div class="container">


          <div class="row">
            <div class="col-lg-5 col-12 mb-2">
              <div class="p-md-4 border-radius">
                <div class="title" data-aos="fade-up">
                  <h4 class="sub-title">Get In</h4>
                  <h2 class="main-title">Touch</h2>
                </div>
                <div class="cntct-page-dtl">
                  <div class="row">
                    <div class="col-12 mb-3 cntct-dtl-row" data-aos="fade-up">
                      <div class="cntct-icon ion-ios-location-outline"></div>
                      <h3>Address</h3>
                      <p>
                        <%-contact.address%>
                      </p>
                    </div>
                    <div class="col-12 mb-3 cntct-dtl-row" data-aos="fade-up">
                      <div class="cntct-icon ion-ios-telephone-outline"></div>
                      <h3>Contact Details</h3>
                      <p>
                        <%=contact.mobile%>
                      </p>
                    </div>
                    <div class="col-12 mb-3 cntct-dtl-row" data-aos="fade-up">
                      <div class="cntct-icon ion-ios-email-outline"></div>
                      <h3>Email Address</h3>
                      <a href="mailto:<%=contact.email%> "><%-contact.email%> </a>
                    </div>

                    <div class="col-12 mb-3 cntct-dtl-row" data-aos="fade-up">
                      <div class="cntct-icon ion-share"></div>
                      <h3>Follow Us</h3>
                      <div class="social">
                        <a href="https://www.facebook.com/profile.php?id=100089324334813" target="_blank"><i
                            class="fa-brands fa-square-facebook"></i></a>
                        <a href="https://www.instagram.com/marutimunchies_/" target="_blank"><i
                            class="fa-brands fa-instagram"></i></a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-lg-7 col-12 mb-2">
              <div class="p-md-4 border-radius">
                <div class="title" data-aos="fade-up">
                  <h4 class="sub-title">Ask Us</h4>
                  <h2 class="main-title">Your Query</h2>
                </div>
                <div class="cntct-form">
                  <form method="post" action="">
                    <div class="row">
                      <div class="col-lg-6 col-md-6 col-sm-12 col-12 mb-4">
                        <label class="form-label">Your Name</label>
                        <input type="text" placeholder="Your Name" name="name" class="form-control form-control-lg"
                          required />
                      </div>
                      <div class="col-lg-6 col-md-6 col-sm-12 col-12 mb-4">
                        <label class="form-label">Your Company Name</label>
                        <input type="text" placeholder="Your Company Name" name="companyname"
                          class="form-control form-control-lg" required />
                      </div>
                      <div class="col-lg-6 col-md-6 col-sm-12 col-12 mb-4">
                        <label class="form-label">Contact Number</label>
                        <input type="text" placeholder="eg : 1234567890" name="contactno"
                          class="form-control form-control-lg" required />
                      </div>
                      <div class="col-lg-6 col-md-6 col-sm-12 col-12 mb-4">
                        <label class="form-label">Email Address</label>
                        <input type="text" placeholder="eg : example@gmail.com" name="email"
                          class="form-control form-control-lg" required />
                      </div>

                      <div class="col-12 mb-4">
                        <label class="form-label">Your Query or Message</label>
                        <textarea placeholder="Your Message" class="form-control form-control-lg" name="message"
                          required></textarea>
                      </div>
                      <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>



        </div>
      </section>

      <section class="map-section">
        <div class="container">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d14768.047046522173!2d70.7922992!3d22.2775443!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3959cb966b5c723d%3A0x9533048cf2de647!2sMaruti%20Munchies!5e0!3m2!1sen!2sin!4v1679408072583!5m2!1sen!2sin"
            allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
      </section>



      <div class="clearfix"></div>
    </div>
    <%- include('layout/footer') %>

    

    <div class="go-top-area">
      <div class="go-top-wrap">
        <div class="go-top-btn-wrap">
          <div class="go-top go-top-btn">
            <i class="fa fa-angle-double-up"></i>
            <i class="fa fa-angle-double-up"></i>
          </div>
        </div>
      </div>
    </div>

  </div>

  <script type="text/javascript" src="js/jquery-3-6-0.js"></script>
  <script type="text/javascript" src="js/bootstrap.bundle.js"></script>
  <script type="text/javascript" src="inc/owl-carosuel/owl.carousel.js"></script>
  <script type="text/javascript" src="inc/fancybox/jquery.fancybox.js"></script>
  <script type="text/javascript" src="inc/anm/anm.js"></script>
  <script type="text/javascript" src="inc/aos/aos.js"></script>
  <script type="text/javascript" src="js/design-common.js"></script>
</body>

</html>